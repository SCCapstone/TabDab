<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.tabdab.HomeFragment"
        android:label="Home"
        tools:layout="@layout/activity_account_information" />

    <fragment
        android:id="@+id/navigation_scanner"
        android:name="com.example.tabdab.ScanFragment"
        android:label="Scan"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.tabdab.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/activity_edit_information" />

    <fragment
        android:id="@+id/navigation_vendor"
        android:name="com.example.tabdab.VendorFragment"
        android:label="Vendor"
        tools:layout="@layout/activity_edit_menu" />

</navigation>
